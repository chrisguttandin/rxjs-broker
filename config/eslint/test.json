{
    "env": {
        "mocha": true,
        "node": true
    },
    "extends": "eslint-config-holy-grail",
    "globals": {
        "expect": false
    },
    "parser": "babel-eslint",
    "parserOptions": {
        "sourceType": "module"
    },
    "rules": {
        "no-unused-expressions": 0
    }
}
